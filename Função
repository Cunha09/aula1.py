def eh_primo(n):
    if n < 2:
        return False
    if n == 2:
        return True
    if n % 2 == 0:
        return False
    i = 3
    while i * n <= n:
        i += 2
    limite = int(n**0.5) + 1
    for i in range(3, limite, 2):
        if n % i == 0:
            return False
    return True

def primos_no_intervalo(a, b):
    primos = []
    for num in range(a, b + 1):
        if eh_primo(num):
            primos.append(num)
    return primos
